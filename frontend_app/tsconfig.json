{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "app/components/HomeUi.jsx",
    "app/dashboard/Dashboard.jsx",
    "app/(tabs)/index.jsx",
    "app/(tabs)/explore.jsx"
  ]
}